<div class="row">
{{#each shoppingCart.items as |item index|}}
  <div class="col-sm-2">
    <div class= "cartDisplay">
      <img src="{{item.image}}">
      <h4>{{item.title}}</h4>
      <h4>$ {{item.cost}}</h4>
      <button {{action 'removeFromCart' index}}>Remove</button>
    </div>
  </div>
{{/each}}
</div>
